{
  "branches": [
    {
      "name": "testing",
      "channel": "pr",
      "prerelease": true,
      "tagFormat": "${version}-PR"
    },
    {
      "name": "main",
      "channel": "release",
      "tagFormat": "${version}-release"
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/npm",
    "@semantic-release/github"
  ]
}
