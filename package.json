    "semantic-release": "^24.2.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.4.2",
    "@types/winston": "^2.4.4",
    "discord-api-types": "^0.38.2",
    "eslint": "^9.26.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1"
